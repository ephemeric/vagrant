# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
    config.vm.define "devaun" do |this|
        this.vm.box = "generic/devaun5"
        this.vm.box_version = "4.3.6"
        this.vm.hostname = "devaun" + DOMAIN
        this.vm.provision :shell, inline: <<-SHELL
            id
        SHELL
    end
end
