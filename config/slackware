# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
    config.vm.define "slackware" do |this|
        this.vm.box = "talosthoren/slackware64-14.2"
        this.vm.box_version = "1.0"
        this.vm.hostname = "slackware" + DOMAIN
        this.vm.provision :shell, inline: <<-SHELL
            id
        SHELL
    end
end
